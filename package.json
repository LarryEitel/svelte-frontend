{
	"name": "@extensionly-gh/svelte-frontend",
	"version": "0.0.1",
	"scripts": {
		"dev": "doppler run -- vite dev --open",
		"build": "doppler run -- vite build",
		"build:ci": "vite build",
		"build:analyze": "ANALYZE=true doppler run -- vite build",
		"preview": "doppler run -- vite preview",
		"preview:ci": "vite preview",
		"test": "pnpm prisma:reset && playwright test",
		"test:ci": "CI=true playwright test",
		"test:gen": "playwright codegen localhost:5173 --load-storage=playwright-logged-in-state.json",
		"test:gen:logged-out": "playwright codegen localhost:5173",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write . && eslint --fix .",
		"prisma:studio": "doppler run -- prisma studio",
		"prisma:push": "doppler run -- prisma db push",
		"prisma:reset": "doppler run -- prisma db push --force-reset && prisma db seed",
		"prisma:ci": "prisma db push --force-reset && prisma db seed",
		"prepare": "husky install && prisma generate && svelte-kit sync",
		"generate-apis": "openapi-generator-cli generate",
		"docker:rm": "docker-compose down",
		"test:db": "docker-compose up",
		"release": "changeset publish"
	},
	"devDependencies": {
		"@auth/core": "^0.2.5",
		"@auth/sveltekit": "^0.3.0",
		"@changesets/changelog-github": "^0.4.8",
		"@changesets/cli": "^2.26.0",
		"@felte/validator-zod": "^1.0.13",
		"@iconify/json": "^2.2.25",
		"@openapitools/openapi-generator-cli": "^2.5.2",
		"@playwright/test": "^1.31.0",
		"@popperjs/core": "^2.11.6",
		"@prisma/client": "4.11.0",
		"@rgossiaux/svelte-headlessui": "^1.0.2",
		"@sveltejs/adapter-node": "^1.2.0",
		"@sveltejs/adapter-vercel": "^2.0.0",
		"@sveltejs/kit": "1.12.0",
		"@tailwindcss/line-clamp": "^0.4.2",
		"@tailwindcss/typography": "^0.5.9",
		"@trpc/client": "^10.12.0",
		"@trpc/server": "^10.12.0",
		"@types/bcrypt": "^5.0.0",
		"@types/google.maps": "^3.52.2",
		"@types/luxon": "^3.2.0",
		"@types/node": "^18.14.0",
		"@types/nprogress": "^0.2.0",
		"@types/ws": "^8.5.4",
		"@typescript-eslint/eslint-plugin": "^5.53.0",
		"@typescript-eslint/parser": "^5.53.0",
		"@zerodevx/svelte-toast": "1.0.0-rc.2",
		"autoprefixer": "^10.4.13",
		"bcrypt": "^5.1.0",
		"class-variance-authority": "^0.4.0",
		"daisyui": "^2.50.2",
		"date-fns": "^2.29.3",
		"eslint": "^8.34.0",
		"eslint-config-prettier": "^8.6.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"felte": "^1.2.7",
		"husky": "^8.0.3",
		"lint-staged": "^13.1.2",
		"luxon": "^3.2.1",
		"nprogress": "^0.2.0",
		"postcss": "^8.4.21",
		"prettier": "^2.8.4",
		"prettier-plugin-svelte": "^2.9.0",
		"prettier-plugin-tailwindcss": "^0.2.4",
		"prisma": "4.11.0",
		"rollup-plugin-visualizer": "^5.9.0",
		"svelte": "^3.55.1",
		"svelte-check": "^3.0.0",
		"svelte-i18n": "^3.6.0",
		"svelte-inview": "^4.0.0",
		"svelte-popperjs": "^1.3.2",
		"svelte-tel-input": "^1.2.0",
		"tailwindcss": "^3.2.7",
		"trpc-sveltekit": "^3.4.2",
		"ts-node": "^10.9.1",
		"tslib": "^2.5.0",
		"typescript": "^5.0.0",
		"unplugin-icons": "^0.15.3",
		"vite": "^4.1.4",
		"ws": "^8.12.1",
		"zod": "^3.20.6"
	},
	"type": "module",
	"prisma": {
		"seed": "prisma/seed.sh"
	},
	"lint-staged": {
		"*{.svelte,.ts,.html,.css,.json,.yml}": [
			"prettier --plugin-search-dir . --write",
			"eslint --fix"
		]
	},
	"engines": {
		"node": ">=18",
		"pnpm": ">=7"
	},
	"packageManager": "pnpm@7.30.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/extensionly-gh/svelte-frontend.git"
	},
	"publishConfig": {
		"registry": "https://npm.pkg.github.com",
		"access": "restricted"
	}
}
